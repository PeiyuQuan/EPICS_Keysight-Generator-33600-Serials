########################General function#############################
record(mbbi,"$(P)$(M):getFunc")
	{
		field(DESC, "Function")
		field(DTYP, "stream")
		field(INP, "@A33600S.proto getFunc($(N)) $(PORT)")
		field(ZRVL, "0")
		field(ONVL, "1")
		field(TWVL, "2")
		field(THVL, "3")
		field(FRVL, "4")
		field(FVVL, "5")
		field(SXVL, "6")
		field(SVVL, "7")
		field(EIVL, "8")
		field(ZRST, "SIN")
		field(ONST, "SQU")
		field(TWST, "TRI")
		field(THST, "RAMP")
		field(FRST, "PULS")
		field(FVST, "PRBS")
		field(SXST, "NOIS")
		field(SVST, "ARB")
		field(EIST, "DC")
		field(SCAN, "1 second")
	}
	
record(mbbo,"$(P)$(M):setFunc")
	{
		field(DESC, "Function")
		field(DTYP, "stream")
		field(OUT, "@A33600S.proto setFunc($(N)) $(PORT)")
		field(ZRVL, "0")
		field(ONVL, "1")
		field(TWVL, "2")
		field(THVL, "3")
		field(FRVL, "4")
		field(FVVL, "5")
		field(SXVL, "6")
		field(SVVL, "7")
		field(EIVL, "8")
		field(ZRST, "SIN")
		field(ONST, "SQU")
		field(TWST, "TRI")
		field(THST, "RAMP")
		field(FRST, "PULS")
		field(FVST, "PRBS")
		field(SXST, "NOIS")
		field(SVST, "ARB")
		field(EIST, "DC")
	}

